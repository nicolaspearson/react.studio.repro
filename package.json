		"mobx": "~5.9.0",
		"mobx-react": "~5.4.3",
		"react": "~16.8.5",
		"react-dom": "~16.8.5",
		"react-particles-js": "~2.5.1",
		"router5": "~7.0.2",
		"router5-plugin-browser": "~7.0.2",
		"typeface-montserrat": "^0.0.54"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "~7.4.0",
		"@craco/craco": "~3.6.0",
		"@types/classnames": "~2.2.7",
		"@types/enzyme": "~3.9.1",
		"@types/enzyme-adapter-react-16": "~1.0.5",

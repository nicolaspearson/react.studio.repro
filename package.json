{
	"name": "react.studio.repro",
	"version": "0.0.1",
	"description": "Website for Studio Repro built using React, Typescript, MobX, Ant Design, and Webpack.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/studio-repro/react.studio.repro"
	},
	"license": "MIT",
	"homepage": "https://studio-repro.github.io/react.studio.repro",
	"scripts": {
		"start": "npm run tslint && craco start",
		"build": "npm run tslint && craco build",
		"cra-start": "react-scripts start",
		"cra-build": "react-scripts build",
		"cra-test": "react-scripts test",
		"cra-eject": "react-scripts eject",
		"prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
		"tslint": "tslint --fix -c tslint.json -p tsconfig.json",
		"test": "jest --verbose --updateSnapshot",
		"test:watch": "jest --watchAll --verbose --updateSnapshot",
		"test:coverage": "jest --verbose --coverage --updateSnapshot",
		"predeploy": "yarn run build",
		"deploy": "gh-pages -d build"
	},
	"dependencies": {
		"@bit/nicolaspearson.interaction.button": "~1.0.2",

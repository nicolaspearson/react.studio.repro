		"@types/node": "~11.11.6",
		"@types/react": "~16.8.8",
		"@types/react-dom": "~16.8.3",
		"craco-antd": "~1.10.0",
		"craco-base64-inline-loader": "~1.0.0",
		"dotenv": "~7.0.0",
		"enzyme": "~3.9.0",

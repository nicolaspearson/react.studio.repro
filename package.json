		"enzyme-to-json": "~3.3.5",
		"gh-pages": "~2.0.1",
		"jest": "~23.6.0",
		"less": "~3.9.0",
		"prettier": "~1.16.4",
		"react-scripts": "~2.1.8",
		"ts-jest": "~24.0.0",
		"tslint": "~5.14.0",
		"tslint-config-prettier": "~1.18.0",
		"tslint-react": "~3.6.0",
		"typescript": "~3.3.4000",
		"webpackbar": "~3.1.5"
